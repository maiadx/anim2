  config=release
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild prelink

ifeq ($(config),release)
  RESCOMP = windres
  TARGETDIR = ../Bin
  TARGET = $(TARGETDIR)/Anim2
  OBJDIR = ../Bin/Int/Anim2/Bin/Anim2/Release
  PCH = Include/pch.h
  GCH = $(OBJDIR)/$(notdir $(PCH)).gch
  DEFINES += -DIMGUI_IMPL_OPENGL_LOADER_GLAD -D_X11 -DJUNO_PLATFORM_LINUX
  INCLUDES += -ISrc -IInclude -I. -IVendor/glm -IVendor/imgui -IVendor/glad/include -IVendor/glfw/include
  FORCE_INCLUDE += -include $(OBJDIR)/$(notdir $(PCH))
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -flto -O3
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -flto -O3 -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += Bin/libGLFW.a Bin/libGLAD.a -lX11 -ldl -lpthread
  LDDEPS += Bin/libGLFW.a Bin/libGLAD.a
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib64 -m64 -flto -s
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

