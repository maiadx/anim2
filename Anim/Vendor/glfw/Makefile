ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = ../../../Bin/Debug
  TARGET = $(TARGETDIR)/libGLFW.a
  OBJDIR = ../../../Bin/Int/Debug/Debug/GLFW
  DEFINES += -D_GLFW_X11
  INCLUDES += -Iinclude
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
