ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = ../Bin/Debug
  TARGET = $(TARGETDIR)/Anim
  OBJDIR = ../Bin/Int/Debug/anim2
  PCH = Src/pch.h
  GCH = $(OBJDIR)/$(notdir $(PCH)).gch
  DEFINES += -D_CRT_SECURE_NO_WARNINGS -DGLFW_INCLUDE_NONE -D_X11 -DANIM_PLATFORM_LINUX -DLOG_BUILD_DEBUG
  INCLUDES += -ISrc -IInclude -I. -IVendor/jpl -IVendor/glm -IVendor/imgui -IVendor/glad/include -IVendor/glfw/include
  FORCE_INCLUDE += -include $(OBJDIR)/$(notdir $(PCH))
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -g
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -g -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += ../Bin/Debug/libGLFW.a ../Bin/Debug/libGLAD.a -lX11 -ldl -lpthread
  LDDEPS += ../Bin/Debug/libGLFW.a ../Bin/Debug/libGLAD.a
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib64 -m64
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

